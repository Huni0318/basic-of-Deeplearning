   "source": [
    "X = [i+1 for i in range(10)]\n",
    "\n",
    "def f(x):\n",
    "    return -np.log(x)\n",
    "\n",
    "max_val = -np.inf\n",
    "argmax = None\n",
    "for x in X:\n",
    "    val = f(x)\n",
    "    if val > max_val:\n",
    "        max_val = val\n",
    "        argmax = x\n",
    "print(argmax)"
   ]
  }
